{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string"
        },
        "tags": {
            "type": "object"
        },
        "vnetConfig": {
            "type": "object"
        },
        "subnetsConfig": {
            "type": "array"
        },
        "nsgsConfig": {
            "type": "array"
        },
        "securityRules": {
            "type": "array"
        },
        "aksConfig": {
            "type": "object"
        },
        "storageName": {
            "type": "string"
        },
        "vaultName": {
            "type": "string"
        },
        "tenantId": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "network-deployment",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/Ductran201/arm-test/main/module/network/network.json"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "tags": {
                        "value": "[parameters('tags')]"
                    },
                    "vnetConfig": {
                        "value": "[parameters('vnetConfig')]"
                    },
                    "subnetsConfig": {
                        "value": "[parameters('subnetsConfig')]"
                    },
                    "nsgsConfig": {
                        "value": "[parameters('nsgsConfig')]"
                    },
                    "securityRules": {
                        "value": "[parameters('securityRules')]"
                    }
                }
            }
        }
    ]
}
